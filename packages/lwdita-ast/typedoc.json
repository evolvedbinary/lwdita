{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "../../docs/ast",
  "tsconfig": "tsconfig.json",
  "exclude": ["**/test/**", "**/node_modules/**", "**/dist/**", "**/generated-docs/**"],
  "treatWarningsAsErrors": true,
  "highlightLanguages": [
      "bash",
      "console",
      "css",
      "html",
      "javascript",
      "json",
      "jsonc",
      "json5",
      "tsx",
      "typescript",
      "xml"
  ],
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true
}